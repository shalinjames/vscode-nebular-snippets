{
  "name": "vscode-nebular-snippets",
  "displayName": "Nebular Code snippets",
  "description": "Code snippents for the Nebular ",
  "version": "0.0.4",
  "publisher": "shalinjames",
  "engines": {
    "vscode": "^1.25.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "keywords": [
    "javascript",
    "html",
    "nebular",
    "snippet",
    "Angular"
  ],
  "activationEvents": [
    "onCommand:extension.sayHello"
  ],
  "main": "./out/extension",
  "contributes": {
    "snippets": [
      {
        "language": "html",
        "path": "./snippets/html.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "tslint": "^5.8.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  },
  "icon": "images/brackets.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/shalinjames/vscode-nebular-snippets.git"
  }
}
