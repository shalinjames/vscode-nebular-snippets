{
  "Nebular button": {
    "prefix": "nb-button",
    "body": ["<button nbButton>${1:text}</button>"]
  },
  "Nebular button disabled": {
    "prefix": "nb-button-disabled",
    "body": ["<button nbButton disabled>${1:text}</button>"]
  },
  "Nebular button colors": {
    "prefix": "nb-button-color",
    "body": ["<button nbButton status=\"primary\">${1:text}</button>"]
  },
  "Nebular button size": {
    "prefix": "nb-button-size",
    "body": ["<button nbButton size=\"medium\">x-small</button>"]
  },
  "Nebular button with outline": {
    "prefix": "nb-button-outline",
    "body": ["<button nbButton outline status=\"primary\">${1:text}</button>"]
  },
  "Nebular hero button": {
    "prefix": "nb-button-hero",
    "body": ["<button nbButton hero status=\"primary\">${1:text}</button>"]
  },
  "Nebular button shape": {
    "prefix": "nb-button-shape",
    "body": [
      "<button nbButton shape=\"${1:semi-round}\" status=\"primary\">${2:text}</button>"
    ]
  },
  "Nebular checkbox": {
    "prefix": "nb-checkbox",
    "body": ["<nb-checkbox>${1:text}</nb-checkbox>"]
  },
  "Nebular Checkbox With Status": {
    "prefix": "nb-checkbox-status",
    "body": ["<nb-checkbox status=\"${2:success}\">${1:Success}</nb-checkbox>"]
  },
  "Nebular Checkbox 2": {
    "prefix": "nb-checkbox-2",
    "body": [
      "<nb-checkbox status=\"${2:success}\">${1:Success}</nb-checkbox>",
      "<nb-checkbox status=\"${4:danger}\">${3:Danger}</nb-checkbox>"
    ]
  },
  "Nebular Checkbox 3": {
    "prefix": "nb-checkbox-3",
    "body": [
      "<nb-checkbox status=\"${2:success}\">${1:Success}</nb-checkbox>",
      "<nb-checkbox status=\"${4:warning}\">${3:Warning}</nb-checkbox>",
      "<nb-checkbox status=\"${6:danger}\">${5:Danger}</nb-checkbox>"
    ]
  }
}
